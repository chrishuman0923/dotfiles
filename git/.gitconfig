[user]
	name = Chris Human
	email = 42072991+chrishuman0923@users.noreply.github.com
[init]
	defaultBranch = main
[pull]
	rebase = false
[core]
	editor = cursor -w
	pager = delta
[interactive]
	diffFilter = delta --color-only
[delta]
	navigate = true
	dark = true
	line-numbers = true
	side-by-side = false
[diff]
	tool = cursor
[difftool "cursor"]
	cmd = "cursor -w --diff  "
[merge]
	tool = cursor
[mergetool "cursor"]
	cmd = "cursor -w "
[commit]
	template = /Users/chuman/.gitmessage
